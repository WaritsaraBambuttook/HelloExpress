<% include ../partials/header %>
<div class="col" style="text-align: center;">
    <h1>ยอดรวมการสั่งซื้อของแต่ละ USER</h1>
</div>
    <table class="table table-striped" id="UsersTB">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(data){ %>
                <tr>
                    <td scope="row">
                        <%=data.name %>
                    </td>
                    <td>
                         <%=data.email %>
                    </td>
                    <td>
                        <%=data.sum %>
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
    <% include ../partials/footer %>