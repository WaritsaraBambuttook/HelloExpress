<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>Pid</th>
            <th>Title</th>
            <th>Price</th>
            <th><a  class="btn btn-primary" href="/addnew" role="button">Add new Product</a></th>     
        </tr>
    </thead>
    <tbody>
    <% products.forEach(function(product){ %>
            <tr>
            <td scope="row"><%=product.id %></td>
            <td><%=product.title %></td>
            <td><%=product.price %></td>
            <td><a  class="btn btn-primary" href="/products/<%= product.id %>" role="button">Edit</a></td>
            </tr>
            
   <% }); %>   
    </tbody>
</table>
<% include ../partials/footer %>
